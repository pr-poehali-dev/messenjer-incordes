      "method": "POST",
      "body": {
        "action": "register",
        "email": "test@incordes.com",
        "password": "testpass123",
        "username": "TestUser"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "token": "string",
        "user": {
          "incordes_id": "string",
          "username": "string",
          "email": "string"
        }
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Login returns JWT token",
      "method": "POST",
      "body": {
        "action": "login",
        "email": "test@incordes.com",
        "password": "testpass123"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "token": "string",
        "user": {
          "incordes_id": "string"
        }
      },
      "bodyMatcher": "partial"
    }
  ]
